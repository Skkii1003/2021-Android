<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.simplealbum">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/album_app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SimpleAlbum">
        <activity android:name=".AlbumActivity"></activity>
        <activity android:name=".ForgetActivity" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".LoginActivity"/>
        <activity
            android:name="com.example.simplealbum.JumpActivity"
            android:icon="@drawable/icon_album"
            android:label="@string/album_app_name">
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>